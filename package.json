{
  "name": "@openlab/wiki-md",
  "description": "Generate a wiki from structured markdown",
  "version": "0.3.0",
  "private": true,
  "repository": "digitalinteraction/wiki-md",
  "author": "",
  "license": "MIT",
  "scripts": {
    "coverage": "npx jest --coverage --collectCoverageFrom 'src/**/*'",
    "dev": "NODE_ENV=development node src/cli.js testdir/ --base-path=dist",
    "lint": "eslint src",
    "prettier": "prettier --write '**/*.{js,json,css,md,ts,tsx}'",
    "preversion": "npm run test -s",
    "start": "node -r dotenv/config src/index.js",
    "test": "jest"
  },
  "keywords": [],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "bulma": "^0.7.4",
    "casex": "^3.0.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.0",
    "dotenv": "^7.0.0",
    "fibers": "^4.0.1",
    "glob": "^7.1.4",
    "gray-matter": "^4.0.2",
    "hast-util-select": "^3.0.1",
    "hast-util-to-text": "^1.0.1",
    "hastscript": "^5.0.0",
    "http-server": "^0.11.1",
    "lodash.debounce": "^4.0.8",
    "ms": "^2.1.1",
    "rehype-document": "^3.0.0",
    "rehype-stringify": "^5.0.0",
    "remark-parse": "^6.0.3",
    "remark-rehype": "^4.0.0",
    "sass": "^1.20.1",
    "unified": "^7.1.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.3.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/sass": "^1.16.0",
    "@types/yargs": "^13.0.0",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.4",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "src/logic.js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "src/index.js"
}
